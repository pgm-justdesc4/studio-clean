{% import "components/general.njk" as components %}
{% set contact = site.contact %}

{% extends "layouts/default.njk" %}

{% block content %}
<main>
    <section class="hero">
            {{ components.hero(title) }}
    </section>

    <section class="contact">
        <div class="container">
            {{ components.contactList(contact) }}
            <button class="button">
                Send us an email
            </button>
            <figure>
                <img src="/assets/images/{{ image }}" alt="Image">
            </figure>
        </div>
    </section>

</main>
{% endblock content %}